let letters = []

let word = "racecar";
let rword = "";

for(let i=0; i<word.length; i++){
  letters.push(word[i])
}
console.log(letters)

for(let i=0; i<word.length; i++){
  rword += letters.pop()
}
console.log(letters)
console.log(rword)

if(rword === word){
  console.log(`${word} is a palindrome.`)
}else{
  console.log(`${word} is not a palidrome`)
}
